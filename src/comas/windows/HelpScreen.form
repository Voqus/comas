                <Property name="text" type="java.lang.String" value="&lt;html&gt;&#xd;&#xa;  &lt;head&gt;&#xd;&#xa;&#xd;&#xa;  &lt;/head&gt;&#xd;&#xa;  &lt;body&gt;&#xd;&#xa;    &lt;p style=&quot;margin-top: 0&quot;&gt;&#xd;&#xa;       &lt;center&gt;&lt;h3&gt;&#x39a;&#x3b1;&#x3c4;&#x3b1;&#x3c7;&#x3ce;&#x3c1;&#x3b9;&#x3c3;&#x3b7; &#x3c0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b7;&lt;/h3&gt;&lt;/center&gt;&#xa;         &#x393;&#x3b9;&#x3b1; &#x3bd;&#x3b1; &#x3b3;&#x3af;&#x3bd;&#x3b5;&#x3b9; &#x3b7; &#x3ba;&#x3b1;&#x3c4;&#x3b1;&#x3c7;&#x3ce;&#x3c1;&#x3b9;&#x3c3;&#x3b7; &#x3b5;&#x3bd;&#x3cc;&#x3c2; &#x3c0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b7;, &#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3ad;&#x3b3;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &lt;b&gt;&#x395;&#x3bd;&#x3ad;&#x3c1;&#x3b3;&#x3b9;&#x3b5;&#x3c2;-&gt;&#x39a;&#x3b1;&#x3c4;&#x3b1;&#x3c7;&#x3ce;&#x3c1;&#x3b9;&#x3c3;&#x3b7; &#x3c0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b7;&lt;/b&gt; &#x3ba;&#x3b1;&#x3b9; &#x3c3;&#x3c5;&#x3bc;&#x3c0;&#x3bb;&#x3b7;&#x3c1;&#x3ce;&#x3bd;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &#x3c4;&#x3b1; &#x3c0;&#x3b5;&#x3b4;&#x3af;&#x3b1;.&#x38c;&#x3bb;&#x3b1; &#x3c4;&#x3b1; &#x3c0;&#x3b5;&#x3b4;&#x3af;&#x3b1; &#x3b5;&#x3af;&#x3bd;&#x3b1;&#x3b9; &#x3c5;&#x3c0;&#x3bf;&#x3c7;&#x3c1;&#x3b5;&#x3c9;&#x3c4;&#x3b9;&#x3ba;&#x3ac;, &#x3ba;&#x3b1;&#x3b9; &#x3ad;&#x3c7;&#x3bf;&#x3c5;&#x3bd; &#x3c3;&#x3c5;&#x3b3;&#x3ba;&#x3b5;&#x3ba;&#x3c1;&#x3b9;&#x3bc;&#x3ad;&#x3bd;&#x3b7; &#x3bc;&#x3bf;&#x3c1;&#x3c6;&#x3ae; &#x3b5;&#x3b9;&#x3c3;&#x3b1;&#x3b3;&#x3c9;&#x3b3;&#x3ae;&#x3c2; &#x3c3;&#x3c4;&#x3bf;&#x3b9;&#x3c7;&#x3b5;&#x3af;&#x3c9;&#x3bd;. &#x3a3;&#x3b5; &#x3ba;&#x3ac;&#x3b8;&#x3b5; &#x3c0;&#x3b5;&#x3b4;&#x3af;&#x3bf; &#x3b5;&#x3ac;&#x3bd; &#x3c3;&#x3cd;&#x3c1;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &#x3c4;&#x3bf;&#x3bd; &#x3ba;&#x3ad;&#x3c1;&#x3c3;&#x3bf;&#x3c1;&#x3b1; &#x3c0;&#x3ac;&#x3bd;&#x3c9; &#x3c4;&#x3bf;&#x3c5; &#x3b8;&#x3b1; &#x3b5;&#x3bc;&#x3c6;&#x3b1;&#x3bd;&#x3b9;&#x3c3;&#x3c4;&#x3b5;&#x3af; &#x3ad;&#x3bd;&#x3b1; &#x3c4;&#x3b5;&#x3c4;&#x3c1;&#x3ac;&#x3b3;&#x3c9;&#x3bd;&#x3bf; &#x3c0;&#x3bf;&#x3c5; &#x3c0;&#x3b5;&#x3c1;&#x3b9;&#x3ad;&#x3c7;&#x3b5;&#x3b9; &#x3ad;&#x3bd;&#x3b1; &#x3c0;&#x3b1;&#x3c1;&#x3ac;&#x3b4;&#x3b5;&#x3b9;&#x3b3;&#x3bc;&#x3b1; &#x3b3;&#x3b9;&#x3b1; &#x3c4;&#x3bf; &#x3c0;&#x3ce;&#x3c2; &#x3bc;&#x3c0;&#x3bf;&#x3c1;&#x3b5;&#x3af; &#x3bd;&#x3b1; &#x3c3;&#x3c5;&#x3bc;&#x3c0;&#x3bb;&#x3b7;&#x3c1;&#x3c9;&#x3b8;&#x3b5;&#x3af; &#x3c4;&#x3bf; &#x3ba;&#x3ac;&#x3b8;&#x3b5; &#x3c0;&#x3b5;&#x3b4;&#x3af;&#x3bf;.  &#x391;&#x3c6;&#x3bf;&#x3cd; &#x3c3;&#x3c5;&#x3bc;&#x3c0;&#x3bb;&#x3b7;&#x3c1;&#x3c9;&#x3b8;&#x3bf;&#x3cd;&#x3bd; &#x3c4;&#x3b1; &#x3c0;&#x3b5;&#x3b4;&#x3af;&#x3b1; &#x3ba;&#x3b1;&#x3b9; &#x3b5;&#x3af;&#x3bc;&#x3b1;&#x3c3;&#x3c4;&#x3b5; &#x3c3;&#x3af;&#x3b3;&#x3bf;&#x3c5;&#x3c1;&#x3bf;&#x3b9; &#x3cc;&#x3c4;&#x3b9; &#x3c4;&#x3b1; &#x3c3;&#x3c4;&#x3bf;&#x3b9;&#x3c7;&#x3b5;&#x3af;&#x3b1; &#x3b5;&#x3af;&#x3bd;&#x3b1;&#x3b9; &#x3c3;&#x3c9;&#x3c3;&#x3c4;&#x3ac; &#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3ad;&#x3b3;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &lt;b&gt;&#x395;&#x3c0;&#x3b9;&#x3b2;&#x3b5;&#x3b2;&#x3b1;&#x3af;&#x3c9;&#x3c3;&#x3b7;&lt;/b&gt;.&lt;br/&gt;&#xa;       &lt;center&gt;&lt;h3&gt;&#x395;&#x3c0;&#x3b5;&#x3be;&#x3b5;&#x3c1;&#x3b3;&#x3b1;&#x3c3;&#x3af;&#x3b1; &#x3c0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b7;&lt;/h3&gt;&lt;/center&gt;&#xa;         &#x393;&#x3b9;&#x3b1; &#x3bd;&#x3b1; &#x3b3;&#x3af;&#x3bd;&#x3b5;&#x3b9; &#x3b7; &#x3b5;&#x3c0;&#x3b5;&#x3be;&#x3b5;&#x3c1;&#x3b3;&#x3b1;&#x3c3;&#x3af;&#x3b1; &#x3b5;&#x3bd;&#x3cc;&#x3c2; &#x3c0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b7;, &#x3ba;&#x3ac;&#x3bd;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &#x3b4;&#x3b9;&#x3c0;&#x3bb;&#x3cc; &#x3ba;&#x3bb;&#x3af;&#x3ba; &#x3c3;&#x3b5; &#x3bc;&#x3af;&#x3b1; &#x3b5;&#x3b3;&#x3b3;&#x3c1;&#x3b1;&#x3c6;&#x3ae; &#x3c3;&#x3c4;&#x3bf;&#x3bd; &#x3c0;&#x3af;&#x3bd;&#x3b1;&#x3ba;&#x3b1; &#x3c0;&#x3b5;&#x3bb;&#x3b1;&#x3c4;&#x3ce;&#x3bd;(&#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3ad;&#x3b3;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &#x3c4;&#x3b7;&#x3bd; &#x3c4;&#x3b1;&#x3bc;&#x3c0;&#x3ad;&#x3bb;&#x3b1; &lt;b&gt;&#x3a0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b5;&#x3c2;&lt;/&gt;). &#x391;&#x3c6;&#x3bf;&#x3cd; &#x3b3;&#x3af;&#x3bd;&#x3b5;&#x3b9; &#x3b7; &#x3b5;&#x3c0;&#x3b5;&#x3be;&#x3b5;&#x3c1;&#x3b3;&#x3b1;&#x3c3;&#x3af;&#x3b1; &#x3bc;&#x3b5; &#x3c4;&#x3b1; &#x3b1;&#x3c0;&#x3b1;&#x3c1;&#x3b1;&#x3af;&#x3c4;&#x3b7;&#x3c4;&#x3b1; &#x3c3;&#x3c4;&#x3bf;&#x3b9;&#x3c7;&#x3b5;&#x3af;&#x3b1;, &#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3ad;&#x3b3;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &lt;b&gt;&#x395;&#x3c0;&#x3b9;&#x3b2;&#x3b5;&#x3b2;&#x3b1;&#x3af;&#x3c9;&#x3c3;&#x3b7;&lt;/b&gt;.&lt;br/&gt;&#xa;       &lt;center&gt;&lt;h3&gt;&#x394;&#x3b9;&#x3b1;&#x3b3;&#x3c1;&#x3b1;&#x3c6;&#x3ae; &#x3b5;&#x3bd;&#x3cc;&#x3c2; &#x3c0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b7;&lt;/h3&gt;&lt;/center&gt;&#xa;         &#x393;&#x3b9;&#x3b1; &#x3bd;&#x3b1; &#x3b3;&#x3af;&#x3bd;&#x3b5;&#x3b9; &#x3b7; &#x3b4;&#x3b9;&#x3b1;&#x3b3;&#x3c1;&#x3b1;&#x3c6;&#x3ae; &#x3b5;&#x3bd;&#x3cc;&#x3c2; &#x3c0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b7;, &#x3ba;&#x3ac;&#x3bd;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &#x3b4;&#x3b9;&#x3c0;&#x3bb;&#x3cc; &#x3ba;&#x3bb;&#x3af;&#x3ba; &#x3c3;&#x3b5; &#x3bc;&#x3af;&#x3b1; &#x3b5;&#x3b3;&#x3b3;&#x3c1;&#x3b1;&#x3c6;&#x3ae; &#x3c3;&#x3c4;&#x3bf;&#x3bd; &#x3c0;&#x3af;&#x3bd;&#x3b1;&#x3ba;&#x3b1; &#x3c0;&#x3b5;&#x3bb;&#x3b1;&#x3c4;&#x3ce;&#x3bd;(&#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3ad;&#x3b3;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &#x3c4;&#x3b7;&#x3bd; &#x3c4;&#x3b1;&#x3bc;&#x3c0;&#x3ad;&#x3bb;&#x3b1; &lt;b&gt;&#x3a0;&#x3b5;&#x3bb;&#x3ac;&#x3c4;&#x3b5;&#x3c2;&lt;/b&gt;), &#x3ba;&#x3b1;&#x3b9; &#x3ad;&#x3c0;&#x3b5;&#x3b9;&#x3c4;&#x3b1; &#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3ad;&#x3b3;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &lt;b&gt;&#x394;&#x3b9;&#x3b1;&#x3b3;&#x3c1;&#x3b1;&#x3c6;&#x3ae;&lt;/b&gt; &#x3b1;&#x3c6;&#x3bf;&#x3cd; &#x3b5;&#x3af;&#x3bc;&#x3b1;&#x3c3;&#x3c4;&#x3b5; &#x3c3;&#x3af;&#x3b3;&#x3bf;&#x3c5;&#x3c1;&#x3bf;&#x3b9; &#x3cc;&#x3c4;&#x3b9; &#x3b5;&#x3af;&#x3bd;&#x3b1;&#x3b9; &#x3b5;&#x3c0;&#x3b9;&#x3b8;&#x3c5;&#x3bc;&#x3b7;&#x3c4;&#x3ae; &#x3b7; &#x3b4;&#x3b9;&#x3b1;&#x3b3;&#x3c1;&#x3b1;&#x3c6;&#x3ae; &#x3c0;&#x3b1;&#x3c4;&#x3ac;&#x3bc;&#x3b5; &lt;b&gt;&#x39d;&#x3b1;&#x3b9;&lt;/b&gt; &#x3c3;&#x3c4;&#x3bf; &#x3c0;&#x3c1;&#x3bf;&#x3b5;&#x3b9;&#x3b4;&#x3bf;&#x3c0;&#x3bf;&#x3b9;&#x3b7;&#x3c4;&#x3b9;&#x3ba;&#x3cc; &#x3bc;&#x3ae;&#x3bd;&#x3c5;&#x3bc;&#x3b1;.&lt;br/&gt;&#xa;       &lt;center&gt;&lt;h3&gt;&#x39a;&#x3b1;&#x3c4;&#x3b1;&#x3c7;&#x3ce;&#x3c1;&#x3b9;&#x3c3;&#x3b7; &#x3c0;&#x3c1;&#x3bf;&#x3bc;&#x3b7;&#x3b8;&#x3b5;&#x3c5;&#x3c4;&#x3ae;&lt;/h3&gt;&lt;/center&gt;&#xa;         &#x393;&#x3b9;&#x3b1; &#x3bd;&#x3b1; &#x3b3;&#x3af;&#x3bd;&#x3b5;&#x3b9; &#x3b7; &#x3ba;&#x3b1;&#x3c4;&#x3b1;&#x3c7;&#x3ce;&#x3c1;&#x3b9;&#x3c3;&#x3b7; &#x3b5;&#x3bd;&#x3cc;&#x3c2; &#x3c0;&#x3c1;&#x3bf;&#x3bc;&#x3b7;&#x3b8;&#x3b5;&#x3c5;&#x3c4;&#x3ae;, &#x3b5;&#x3c0;&#x3b9;&#x3bb;&#x3ad;&#x3b3;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &lt;b&gt;&#x395;&#x3bd;&#x3ad;&#x3c1;&#x3b3;&#x3b9;&#x3b5;&#x3c2;-&gt;&#x39a;&#x3b1;&#x3c4;&#x3b1;&#x3c7;&#x3ce;&#x3c1;&#x3b9;&#x3c3;&#x3b7; &#x3c0;&#x3c1;&#x3bf;&#x3bc;&#x3b7;&#x3b8;&#x3b5;&#x3c5;&#x3c4;&#x3ae;&lt;/b&gt; &#x3ba;&#x3b1;&#x3b9; &#x3c3;&#x3c5;&#x3bc;&#x3c0;&#x3bb;&#x3b7;&#x3c1;&#x3ce;&#x3bd;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &#x3c4;&#x3b1; &#x3c0;&#x3b5;&#x3b4;&#x3af;&#x3b1;.&#x38c;&#x3bb;&#x3b1; &#x3c4;&#x3b1; &#x3c0;&#x3b5;&#x3b4;&#x3af;&#x3b1; &#x3b5;&#x3af;&#x3bd;&#x3b1;&#x3b9; &#x3c5;&#x3c0;&#x3bf;&#x3c7;&#x3c1;&#x3b5;&#x3c9;&#x3c4;&#x3b9;&#x3ba;&#x3ac;, &#x3ba;&#x3b1;&#x3b9; &#x3ad;&#x3c7;&#x3bf;&#x3c5;&#x3bd; &#x3c3;&#x3c5;&#x3b3;&#x3ba;&#x3b5;&#x3ba;&#x3c1;&#x3b9;&#x3bc;&#x3ad;&#x3bd;&#x3b7; &#x3bc;&#x3bf;&#x3c1;&#x3c6;&#x3ae; &#x3b5;&#x3b9;&#x3c3;&#x3b1;&#x3b3;&#x3c9;&#x3b3;&#x3ae;&#x3c2; &#x3c3;&#x3c4;&#x3bf;&#x3b9;&#x3c7;&#x3b5;&#x3af;&#x3c9;&#x3bd;. &#x3a3;&#x3b5; &#x3ba;&#x3ac;&#x3b8;&#x3b5; &#x3c0;&#x3b5;&#x3b4;&#x3af;&#x3bf; &#x3b5;&#x3ac;&#x3bd; &#x3c3;&#x3cd;&#x3c1;&#x3bf;&#x3c5;&#x3bc;&#x3b5; &#x3c4;&#x3bf;&#x3bd; &#x3ba;&#x3ad;&#x3c1;&#x3c3;&#x3bf;&#x3c1;&#x3b1; &#x3c0;&#x3ac;&#x3bd;&#x3c9; &#x3c4;&#x3bf;&#